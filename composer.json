{
    "name": "joostvanveen/laravel-postcode-nl",
    "description": "A Laravel client using the Postcode.nl REST API for Dutch address verification. Forked from https://github.com/Speelpenning-nl/laravel-postcode-nl",
    "license": "MIT",
    "authors": [
        {
            "name": "Gerben Speelpenning",
            "email": "gerben@speelpenning.nl"
        },
        {
            "name": "Joost van Veen",
            "email": "joost@accentinteractive.nl"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/config": "5.8.*",
        "illuminate/contracts": "5.8.*",
        "illuminate/routing": "5.8.*",
        "illuminate/support": "5.8.*",
        "illuminate/validation": "5.8.*"
    },
    "require-dev": {
        "laravel/laravel": "5.8.*",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Speelpenning\\PostcodeNl\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
